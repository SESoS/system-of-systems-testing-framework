<?xml version="1.0" encoding="utf-8"?>
<Diagram type="Agent">
  <Actor name="Driver">
    <arrow name="transferPatient" to="Ambulance" />
  </Actor>
  <Agent name="Ambulance">
    <attribute type="plays" name="MobileCareUnit" />
    <attribute type="possesses" name="TransferPatient" />
    <attribute type="possesses" name="FindSuitableHospital" />
    <arrow name="driveAmbulance" to="Driver" />
    <arrow name="receivePatient" to="Hospital" />
  </Agent>
  <Agent name="Hospital">
    <attribute type="plays" name="MedicalCenter" />
    <attribute type="possesses" name="TreatPatient" />
  </Agent>
  <Agent name="EmergencyCallCenter">
    <attribute type="plays" name="EmergencyControl" />
    <attribute type="possesses" name="ProcessHelpRequest" />
    <arrow name="pickupPatient" to="Ambulance" />
    <arrow name="talkToPatient" to="CallReceiver" />
    <arrow name="waitForPickup" to="Patient" />
  </Agent>
  <Actor name="CallReceiver">
    <arrow name="assignAmbulance" to="EmergencyCallCenter" />
  </Actor>
  <Actor name="Patient">
    <arrow name="waitForPickup" to="Patient" />
  </Actor>
</Diagram>