<?xml version="1.0" encoding="utf-8"?>
<Diagram type="Agent">
  <Actor name="MCI_Reporter">
    <arrow name="activateMCI" to="EmergencyCallCenter" />
  </Actor>
  <Agent name="EmergencyCallCenter">
    <attribute type="plays" name="CommunicationCenter" />
    <attribute type="plays" name="IncidentCommander" />
    <attribute type="possesses" name="ProcessHelpRequest" />
    <attribute type="possesses" name="BroadcastVehicle" />
    <arrow name="reportMCI" to="EmergencyCallCenter" />
    <arrow name="assignAmbulance" to="EmergencyCallCenter" />
    <arrow name="pickupPatient" to="Ambulance" />
  </Agent>
  <Agent name="Ambulance">
    <attribute type="plays" name="EmergencyVehicle" />
    <attribute type="plays" name="EmergencyServices" />
    <attribute type="possesses" name="GetPatientStatus" />
    <attribute type="possesses" name="FindSuitableHospital" />
    <attribute type="possesses" name="PerformFirstAid"></attribute>
    <arrow name="performPrimaryTriage" to="Ambulance" />
    <arrow name="receivePatient" to="Hospital" />
  </Agent>
  <Agent name="Hospital">
    <attribute type="plays" name="MedicalCenter" />
    <attribute type="possesses" name="CurePatient" />
    <arrow name="reportSavedPatient" to="EmergencyCallCenter" />
  </Agent>
</Diagram>